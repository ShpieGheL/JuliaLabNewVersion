<Window x:Class="JuliaUpgrade2._0.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JuliaUpgrade2._0"
        mc:Ignorable="d"
        Title="Добавить вид работы" MaxHeight="125" MinHeight="125" MinWidth="700" MaxWidth="1000">
    <Grid Name="G1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Name="Tab1L1" FontSize="20" Content="Название:"/>
        <TextBox Name="Tab1T1" Grid.ColumnSpan="2" Margin="100,0,0,0" FontSize="20"/>
        <Label Name="Tab1L2" Grid.Row="1" FontSize="20" Content="Стимость (Рублей):"/>
        <TextBox Name="Tab1T2" Grid.Row="1" HorizontalAlignment="Right" Width="100" FontSize="20"/>
        <Button Name="B1" Grid.Column="1" Grid.Row="1" FontSize="20" Content="Добавить" Click="Button_Click"/>
    </Grid>
</Window>
